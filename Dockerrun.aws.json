{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "jmp119/nft-toolkit-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"],
      "memory": 100
    },
    {
      "name": "client",
      "image": "jmp119/nft-toolkit-client",
      "hostname": "client",
      "essential": false,
      "memory": 300
    },
    {
      "name": "server",
      "image": "jmp119/nft-toolkit-server",
      "hostname": "server",
      "essential": false,
      "memory": 300
    }
  ]
}
