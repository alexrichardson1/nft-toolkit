{
  "name": "nft-toolkit-server",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "main": "build/src/",
  "scripts": {
    "build": "tsc",
    "test": "yarn jest src/tests/",
    "start": "nodemon --files src/server.ts",
    "start-production": "node .",
    "compile": "yarn hardhat compile",
    "console": "yarn hardhat console",
    "soltest": "yarn hardhat test"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.0.0",
    "@ethersproject/bytes": "^5.0.0",
    "@ethersproject/providers": "^5.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "4.3.2",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/chai": "^4.2.22",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/mocha": "^9.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.9.2",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.6.5",
    "jest": "^27.3.1",
    "lodash": "^4.17.21",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.13",
    "set-value": "^4.0.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.2.1",
    "typechain": "^5.2.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "build"
    ],
    "coveragePathIgnorePatterns": [
      "smart-contracts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.37.0",
    "@aws-sdk/signature-v4-crt": "^3.31.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongodb": "^4.1.3",
    "mongoose": "^6.0.11",
    "multer": "^1.4.3",
    "underscore": "^1.12.1"
  }
}
